{
  "name": "eslint-plugin-vality",
  "description": "ESLint plugin for Vality.",
  "version": "1.2.8",
  "author": "Jesper Engberg <jeengbe@gmail.com>",
  "homepage": "https://ts-vality.io/eslint",
  "repository": {
    "type": "git",
    "url": "git://github.com/jeengbe/vality.git",
    "directory": "packages/eslint-plugin-vality"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "vality"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeengbe/vality/issues"
  },
  "dependencies": {
    "@typescript-eslint/utils": "5.38.1"
  },
  "devDependencies": {
    "@jest/types": "29.1.2",
    "@swc/core": "1.3.4",
    "@swc/jest": "0.2.23",
    "@types/eslint": "8.4.6",
    "@types/jest": "29.1.1",
    "@types/node": "18.7.23",
    "@typescript-eslint/parser": "5.38.1",
    "eslint": "8.24.0",
    "jest": "29.1.2",
    "typescript": "4.8.4"
  },
  "scripts": {
    "build": "tsc -p src/tsconfig.cjs.json & tsc -p src/tsconfig.mjs.json",
    "lint": "echo \"Error: no linting configured\"",
    "test": "jest tests --ci & tsc --noEmit -p tests/tsconfig.json",
    "typecheck": "tsc --noEmit -p src/tsconfig.cjs.json & tsc --noEmit -p src/tsconfig.mjs.json",
    "test:coverage": "pnpm run test --coverage",
    "test:watch": "jest --watchAll --coverage tests"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "5.38.0",
    "eslint": "8.23.1",
    "vality": "*"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
	"exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/cjs/*.js"
      },
      "import": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "default": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      }
    },
    "./package.json": "./package.json"
	},
  "typings": "dist/typings/index.d.ts",
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
